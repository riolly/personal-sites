<style>
	#star-1 {
		animation: star-blinking 4s infinite ease-in-out backwards;
	}
	#star-2 {
		animation: star-blinking 4s 1s infinite ease-in-out backwards;
	}
	#star-3 {
		animation: star-blinking 4s 3s infinite ease-in-out backwards;
	}
	#star-4 {
		animation: star-blinking 4s 2s infinite ease-in-out backwards;
	}
	#star-5 {
		animation: star-blinking 4s infinite ease-in-out backwards;
	}

	@keyframes star-blinking {
		0% {
			opacity: 0;
		}
		20% {
			opacity: 0.6;
		}
		30% {
			opacity: 0.4;
		}
		40%,
		60% {
			opacity: 1;
		}
		70% {
			opacity: 0.5;
		}
		70% {
			opacity: 0.8;
		}
		100% {
			opacity: 0;
		}
	}
</style>
